{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2019",
    "module": "esnext",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    //"noUnusedLocals": true,
    "noUnusedParameters": true,
    "lib": ["dom", "esnext"],
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "."
  },
  "exclude": [
    "node_modules",
    "dist",
    "test"
  ]
}
